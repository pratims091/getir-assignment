{
  "name": "getir-assignment",
  "description": "Simple REST API using express and mongoDB",
  "version": "2.0.0",
  "author": "Pratim Sarangi<https://github.com/pratims091>",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": "14.17.1",
    "npm": "7.20.5"
  },
  "scripts": {
    "build": "babel src --out-dir dist --copy-files",
    "start": "node ./dist/server.js",
    "dev": "NODE_ENV=development nodemon --exec babel-node -- -r dotenv/config ./src/server.js",
    "dev-debug": "NODE_ENV=development nodemon --exec babel-node --inspect -- ./src/server.js",
    "lint": "npx eslint --cache . --ext js",
    "lintfix": "npx eslint --cache . --ext js --fix",
    "prestart": "npm install --production",
    "prepare": "husky install",
    "test": "jest"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "captains-log": "^2.0.3",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.0",
    "helmet": "^3.21.1",
    "lodash": "^4.17.14",
    "mongodb": "^4.1.2",
    "swagger-ui-express": "^4.1.6"
  },
  "lint-staged": {
    "*.{js, }": [
      "npm run lint"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/node": "^7.14.7",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@jest/globals": "^27.2.0",
    "eslint": "^7.30.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4",
    "husky": "^7.0.1",
    "jest": "^27.2.0",
    "lint-staged": "^11.0.1",
    "nodemon": "^2.0.12",
    "superagent": "^6.1.0",
    "supertest": "^6.1.6"
  }
}
